/* -----------------------------------------
   Font-face:
----------------------------------------- */

@font-face{ 
font-family:"Open_Sans_B";
src: url("<%= asset_path 'fonts/OpenSans-Bold-webfont.eot' %>"); 
src: 
url("<%= asset_path 'fonts/OpenSans-Bold-webfont.eot?#iefix' %>") format('embedded-opentype'), 
url("<%= asset_path 'fonts/OpenSans-Bold-webfont.woff' %>") format('woff'), 
url("<%= asset_path 'fonts/OpenSans-Bold-webfont.ttf' %>") format('truetype'), 
url("<%= asset_path 'fonts/OpenSans-Bold-webfont.svg#webfont' %>") format('svg'); 
}
@font-face{ 
font-family:"Open_Sans_R";
src: url("<%= asset_path 'fonts/OpenSans-Regular-webfont.eot' %>"); 
src: 
url("<%= asset_path 'fonts/OpenSans-Regular-webfont.eot?#iefix' %>") format('embedded-opentype'), 
url("<%= asset_path 'fonts/OpenSans-Regular-webfont.woff' %>") format('woff'), 
url("<%= asset_path 'fonts/OpenSans-Regular-webfont.ttf' %>") format('truetype'), 
url("<%= asset_path 'fonts/OpenSans-Regular-webfont.svg#webfont' %>") format('svg'); 
}
