url("<%= asset_path 'fonts/OpenSans-Light-webfont.woff' %>") format('woff'), 
url("<%= asset_path 'fonts/OpenSans-Light-webfont.ttf' %>") format('truetype'), 
url("<%= asset_path 'fonts/OpenSans-Light-webfont.svg#webfont' %>") format('svg'); 
}


@font-face{ 
font-family:"sosa";
src: url("<%= asset_path 'fonts/sosa.eot' %>"); 
src: 
url("<%= asset_path 'fonts/sosa.eot?#iefix' %>") format('embedded-opentype'), 
url("<%= asset_path 'fonts/sosa.woff' %>") format('woff'), 
url("<%= asset_path 'fonts/sosa.ttf' %>") format('truetype'), 
url("<%= asset_path 'fonts/sosa.svg#webfont' %>") format('svg'); 
